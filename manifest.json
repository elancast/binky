{
    "name": "Binky: Bing Keyboard Shortcuts",
    "version": "1.0",
    "manifest_version": 2,

    // Recommended
    "description": "Gives you the power of keyboard shortcuts for Bing",
    "icons": {
	"16" : "icon16.png",
	"48" : "icon48.png",
	"128" : "icon.png"
    }, //http://code.google.com/chrome/extensions/manifest.html#icons

    // Pick one (or none)
    /*
    "browser_action": {
      ...},
  "page_action": {
      ...},
  "theme": {
      ...},
  "app": {
      ...},
     */

    // Add any of these that you need
    //"background": {...},
    "content_scripts": [ {
			     "matches": [ "http://www.bing.com/search*" ],
			     "css": [],
			     "js": [ "shortcuts.js" ]
			 } ]
}
